<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.2"
      fx:controller="com.example.trabalhofinal.controller.MainController">
    <MenuBar>
        <Menu mnemonicParsing="false" text="%label.arquivo">
            <MenuItem mnemonicParsing="false" onAction="#sair" text="Fechar"/>
        </Menu>
        <Menu mnemonicParsing="false" text="%label.administracao">
            <MenuItem mnemonicParsing="false" onAction="#usuarios" text="%label.usuarios"/>
            <MenuItem mnemonicParsing="false" onAction="#cardapio" text="%label.cardapio"/>
        </Menu>
    </MenuBar>
    <TabPane fx:id="tabPane" tabClosingPolicy="ALL_TABS">
    </TabPane>
</VBox>
